                                {% if app.user %}
                                    <li><a href="/logout"><i class="fa fa-circle-o"></i>Logout</a></li>
                                {% else %}
                                    <li {% if route_name == "login" %} class="active"{% endif %}><a href="/login"><i class="fa fa-circle-o"></i>Login</a></li>
                                {% endif %}
                                    <li {% if route_name == "default_find" %} class="active"{% endif %}><a href="{{ path('default_find') }}"><i class="fa fa-circle-o"></i>findPage</a></li>
                                    <li {% if route_name == "default_test" %} class="active"{% endif %}><a href="{{ path('default_test') }}"><i class="fa fa-circle-o"></i>testPage</a></li>
                                    <li {% if route_name == "default_home" %} class="active"{% endif %}><a href="{{ path('default_home') }}"><i class="fa fa-circle-o"></i>Home Page</a></li>
                                    <li {% if route_name in ["server_list", "server_edit"] %} class="active"{% endif %}><a href="{{ path('server_list') }}"><i class="fa fa-circle-o"></i>FTP-Import pausieren</a></li>
                                    <li {% if route_name in ["server_pause_list"] %} class="active"{% endif %}><a href="{{ path('server_pause_list') }}"><i class="fa fa-circle-o"></i>Pausierte FTP-Importe </a></li>
                                <br>
                                
                                {% if is_granted("ROLE_STATISTIC") %}
                                    <li {% if route_name == "statistic_dashboard" %} class="active"{% endif %}><a href="{{ path('statistic_dashboard') }}"><i class="fa fa-circle-o"></i>Statistic</a></li>
                                {% endif %}
                                <br>
                                {% if is_granted("ROLE_SUPPORT") %}
                                    <li {% if route_name == "makler_new" %} class="active"{% endif %}><a href="{{ path('makler_new') }}"><i class="fa fa-circle-o"></i>Makler hinzufügen</a></li>
		                            <li {% if route_name in ["makler_list", "makler_edit", "makler_ftp_edit", "makler_pw_edit"]  %} class="active"{% endif %}><a href="{{ path('makler_list') }}"><i class="fa fa-circle-o"></i>Makler bearbeiten</a></li>
		                            <li {% if route_name in ["makler_delete_list", "makler_delete"]  %} class="active"{% endif %}><a href="{{ path('makler_delete_list') }}"><i class="fa fa-circle-o"></i>Makler löschen</a></li>
		                            <li {% if route_name in ["interessent_list", "interessent_edit", "interessent_pw_edit"] %} class="active"{% endif %}><a href="{{ path('interessent_list') }}"><i class="fa fa-circle-o"></i>Interessent bearbeiten</a></li>
		                            <li {% if route_name in ["interessent_delete_list", "interessent_delete"] %} class="active"{% endif %}><a href="{{ path('interessent_delete_list') }}"><i class="fa fa-circle-o"></i>Interessent löschen</a></li>
                                    <li {% if route_name in ["bcuser_list", "bcuser_edit"] %} class="active"{% endif %}><a href="{{ path('bcuser_list') }}"><i class="fa fa-circle-o"></i>BusinessClub User bearbeiten</a></li>
                                    <li {% if route_name in ["bcuser_delete_list", "bcuser_delete"] %} class="active"{% endif %}><a href="{{ path('bcuser_delete_list') }}"><i class="fa fa-circle-o"></i>BusinessClub User löschen</a></li>
                                    {# <li {% if route_name in ["server_list"] %} class="active"{% endif %}><a href="{{ path('server_list') }}"><i class="fa fa-circle-o"></i>Server Config</a></li>   #}
