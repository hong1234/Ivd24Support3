{% extends 'baseList.html.twig' %}

{% block title %}BClub-User List{% endblock %}

{% block content %}
    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <section class="content-header">
            <h1>BusinessClub-User löschen</h1>
        </section>
        <!-- Main content -->
        <section class="content">
            <div class="row">
                <div class="col-xs-12">
                    <div class="box">
                        <div class="box-header">
                            <h3 class="box-title">Übersicht - Folgende BClub-User stehen zur Löschung an:</h3>
                        </div>
                        <!-- /.box-header -->
                        <div class="box-body">
                            <table id="example1" class="table table-bordered table-striped"  width="100%">
                                <thead>
                                    <tr>
                                        <th style='width: 8%;'>User-ID</th>
                                        <th style='width: 8%;'>MitgliedNr</th>
                                        <th style='width: 16%;'>Firma</th>
                                        <th style='width: 16%;'>E-Mail</th>
                                        <th style='width: 12%;'>Paket</th>
                                        <th style='width: 8%;'>Start_abo</th>   
                                        <th style='width: 8%;'>End_abo</th>
                                        <th style='width: 8%;'>Status</th>
                                        <th style='width: 16%;'>&nbsp;</th>
                                    </tr>
                                </thead> 
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->
{% endblock %}