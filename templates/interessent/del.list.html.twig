{% extends 'baseList.html.twig' %}

{% block title %}Interessent Delete{% endblock %}

{% block content %}
    <!-- Content Wrapper. Contains page content -->
    <div class="content-wrapper">
        <section class="content-header">
            <h1>Interessent löschen</h1>
        </section>
        <!-- Main content -->
        <section class="content">
            <div class="row">
                <div class="col-xs-12">
                    <div class="box">
                        <div class="box-header">
                            <h3 class="box-title">Übersicht - Folgende Interessenten stehen zur Löschung an:</h3>
                        </div>
                        <!-- /.box-header -->
                        <div class="box-body">
                            <table id="example1" class="table table-bordered table-striped" width="100%">
                                <thead>
                                    <tr>
                                        <th style='width: 8%;'>User-ID</th>
                                        <th style='width: 20%;'>Vorname, Name</th>
                                        <th style='width: 22%;'>Firma</th>
				                        <th style='width: 22%;'>E-Mail</th>
                                        <th style='width: 8%;'>Löschdatum</th>
                                        <th style='width: 20%;'>&nbsp;</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
    <!-- /.content-wrapper -->
{% endblock %}